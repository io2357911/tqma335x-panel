counter = 1;

while (1) {
	if (tag("_D4") == 0) {
		log("Условие выхода выполнено");
		break;
	}

	setTag("Z1OP", 1);
	log("Установка Z1OP в 1");
	wait(1000);

	setTag("Z1OP", 0);
	log("Установка Z1OP в 0");

	log("Конец цикла " + counter);
	setCounter(counter);

	wait(1000);

	counter = counter + 1;
}

log("Испытание завершено");
setStatus("Завершено успешно");
